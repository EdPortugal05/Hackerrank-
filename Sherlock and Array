using System;
using System.Text.RegularExpressions;

class Solution
{
    static void Main()
        {
                // Read the entire stdin and split by any whitespace (spaces, newlines, tabs).
                        // This makes input parsing robust even if there are blank lines or extra spaces.
                                string all = Console.In.ReadToEnd();
                                
                                        // If input is empty, nothing to do.
                                                if (string.IsNullOrWhiteSpace(all)) return;
                                                
                                                        // Split by one-or-more whitespace characters into tokens.
                                                                string[] tokens = Regex.Split(all.Trim(), @"\s+");
                                                                        int idx = 0;
                                                                        
                                                                                // Number of test cases
                                                                                        int T = int.Parse(tokens[idx++]);
                                                                                        
                                                                                                for (int tc = 0; tc < T; tc++)
                                                                                                        {
                                                                                                                    // Read n (size of array)
                                                                                                                                int n = int.Parse(tokens[idx++]);
                                                                                                                                
                                                                                                                                            // Read the next n integers into the array
                                                                                                                                                        long[] arr = new long[n];
                                                                                                                                                                    long total = 0;
                                                                                                                                                                                for (int i = 0; i < n; i++)
                                                                                                                                                                                            {
                                                                                                                                                                                                            arr[i] = long.Parse(tokens[idx++]);
                                                                                                                                                                                                                            total += arr[i];          // accumulate total sum while reading
                                                                                                                                                                                                                                        }
                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                    // Now check for an index where left sum == right sum
                                                                                                                                                                                                                                                                long leftSum = 0;
                                                                                                                                                                                                                                                                            bool found = false;
                                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                                                        for (int i = 0; i < n; i++)
                                                                                                                                                                                                                                                                                                    {
                                                                                                                                                                                                                                                                                                                    // rightSum = total - leftSum - arr[i]
                                                                                                                                                                                                                                                                                                                                    long rightSum = total - leftSum - arr[i];
                                                                                                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                                                                                                                    if (leftSum == rightSum)
                                                                                                                                                                                                                                                                                                                                                                    {
                                                                                                                                                                                                                                                                                                                                                                                        found = true;
                                                                                                                                                                                                                                                                                                                                                                                                            break;
                                                                                                                                                                                                                                                                                                                                                                                                                            }
                                                                                                                                                                                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                                                                                                                                                                                                            leftSum += arr[i]; // move current element to left sum
                                                                                                                                                                                                                                                                                                                                                                                                                                                        }
                                                                                                                                                                                                                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                                                                                                                                                                                                                                    Console.WriteLine(found ? "YES" : "NO");
                                                                                                                                                                                                                                                                                                                                                                                                                                                                            }
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                }
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                }
