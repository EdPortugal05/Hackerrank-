using System;

class Solution
{
    static void Main()
        {
                int q = int.Parse(Console.ReadLine());
                        for (int i = 0; i < q; i++)
                                {
                                            uint n = uint.Parse(Console.ReadLine());
                                                        Console.WriteLine(~n); // flip bits
                                                                }
                                                                    }
                                                                    }
